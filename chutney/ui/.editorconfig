# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.html]
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.ts]
quote_type = single